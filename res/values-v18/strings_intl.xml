<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--
		Since Android 4.3, services with startForeground() are forced to 
		show a notification so we have to enable notification by default
		so that the system wont kill Locker.
    -->
    <string name="pref_def_show_notification">true</string>
    <string name="pref_def_hide_notification_icon">true</string>

</resources>